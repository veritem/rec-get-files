#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/veritem/dev/dir-walk/node_modules/.pnpm/ts-jest@27.0.5_fe15ac987db2e7d4ad38346689ae2fc5/node_modules/ts-jest/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/ts-jest@27.0.5_fe15ac987db2e7d4ad38346689ae2fc5/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/node_modules:/home/veritem/dev/dir-walk/node_modules:/home/veritem/dev/node_modules:/home/veritem/node_modules:/home/node_modules:/node_modules:/home/veritem/dev/dir-walk/node_modules/ts-jest/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/veritem/dev/dir-walk/node_modules/.pnpm/ts-jest@27.0.5_fe15ac987db2e7d4ad38346689ae2fc5/node_modules/ts-jest/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/ts-jest@27.0.5_fe15ac987db2e7d4ad38346689ae2fc5/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/node_modules:/home/veritem/dev/dir-walk/node_modules:/home/veritem/dev/node_modules:/home/veritem/node_modules:/home/node_modules:/node_modules:/home/veritem/dev/dir-walk/node_modules/ts-jest/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
