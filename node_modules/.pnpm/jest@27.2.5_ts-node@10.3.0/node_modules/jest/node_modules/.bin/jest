#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/veritem/dev/dir-walk/node_modules/.pnpm/jest@27.2.5_ts-node@10.3.0/node_modules/jest/bin/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/jest@27.2.5_ts-node@10.3.0/node_modules/jest/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/jest@27.2.5_ts-node@10.3.0/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/node_modules:/home/veritem/dev/dir-walk/node_modules:/home/veritem/dev/node_modules:/home/veritem/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/veritem/dev/dir-walk/node_modules/.pnpm/jest@27.2.5_ts-node@10.3.0/node_modules/jest/bin/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/jest@27.2.5_ts-node@10.3.0/node_modules/jest/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/jest@27.2.5_ts-node@10.3.0/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/node_modules:/home/veritem/dev/dir-walk/node_modules:/home/veritem/dev/node_modules:/home/veritem/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
