#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+parser@7.15.8/node_modules/@babel/parser/bin/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+parser@7.15.8/node_modules/@babel/parser/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+parser@7.15.8/node_modules/@babel/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+parser@7.15.8/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/node_modules:/home/veritem/dev/dir-walk/node_modules:/home/veritem/dev/node_modules:/home/veritem/node_modules:/home/node_modules:/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/jest-snapshot@27.2.5/node_modules/jest-snapshot/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/jest-snapshot@27.2.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+parser@7.15.8/node_modules/@babel/parser/bin/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+parser@7.15.8/node_modules/@babel/parser/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+parser@7.15.8/node_modules/@babel/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+parser@7.15.8/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/node_modules:/home/veritem/dev/dir-walk/node_modules:/home/veritem/dev/node_modules:/home/veritem/node_modules:/home/node_modules:/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/jest-snapshot@27.2.5/node_modules/jest-snapshot/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/jest-snapshot@27.2.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@babel+parser@7.15.8/node_modules/@babel/parser/bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../../../../../@babel+parser@7.15.8/node_modules/@babel/parser/bin/babel-parser.js" "$@"
fi
