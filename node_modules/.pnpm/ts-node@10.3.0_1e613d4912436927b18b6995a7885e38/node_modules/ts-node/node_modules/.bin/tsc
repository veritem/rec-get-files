#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/veritem/dev/dir-walk/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/bin/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/typescript@4.4.4/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/node_modules:/home/veritem/dev/dir-walk/node_modules:/home/veritem/dev/node_modules:/home/veritem/node_modules:/home/node_modules:/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/ts-node@10.3.0_1e613d4912436927b18b6995a7885e38/node_modules/ts-node/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/ts-node@10.3.0_1e613d4912436927b18b6995a7885e38/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/veritem/dev/dir-walk/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/bin/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/typescript@4.4.4/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/node_modules:/home/veritem/dev/dir-walk/node_modules:/home/veritem/dev/node_modules:/home/veritem/node_modules:/home/node_modules:/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/ts-node@10.3.0_1e613d4912436927b18b6995a7885e38/node_modules/ts-node/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/ts-node@10.3.0_1e613d4912436927b18b6995a7885e38/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.4.4/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.4.4/node_modules/typescript/bin/tsc" "$@"
fi
