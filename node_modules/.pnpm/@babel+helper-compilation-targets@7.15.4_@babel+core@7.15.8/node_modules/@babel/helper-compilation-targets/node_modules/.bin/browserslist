#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/veritem/dev/dir-walk/node_modules/.pnpm/browserslist@4.17.4/node_modules/browserslist/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/browserslist@4.17.4/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/node_modules:/home/veritem/dev/dir-walk/node_modules:/home/veritem/dev/node_modules:/home/veritem/node_modules:/home/node_modules:/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+helper-compilation-targets@7.15.4_@babel+core@7.15.8/node_modules/@babel/helper-compilation-targets/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+helper-compilation-targets@7.15.4_@babel+core@7.15.8/node_modules/@babel/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+helper-compilation-targets@7.15.4_@babel+core@7.15.8/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/veritem/dev/dir-walk/node_modules/.pnpm/browserslist@4.17.4/node_modules/browserslist/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/browserslist@4.17.4/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/node_modules:/home/veritem/dev/dir-walk/node_modules:/home/veritem/dev/node_modules:/home/veritem/node_modules:/home/node_modules:/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+helper-compilation-targets@7.15.4_@babel+core@7.15.8/node_modules/@babel/helper-compilation-targets/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+helper-compilation-targets@7.15.4_@babel+core@7.15.8/node_modules/@babel/node_modules:/home/veritem/dev/dir-walk/node_modules/.pnpm/@babel+helper-compilation-targets@7.15.4_@babel+core@7.15.8/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../browserslist@4.17.4/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../browserslist@4.17.4/node_modules/browserslist/cli.js" "$@"
fi
