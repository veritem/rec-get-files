{
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "outDir": "./dist",
    "moduleResolution": "node",
    "lib": ["ES2020"],
    "rootDir": "./",
    "strict": true,
    "declaration": true,
    "pretty": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node", "jest"]
  }
}
